{
  "name": "@a2akit/core",
  "version": "0.2.0",
  "description": "A TypeScript library for creating A2A protocol servers with decorators",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.js"
      },
      "require": {
        "types": "./dist/server/index.d.cts",
        "default": "./dist/server/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a2akit/a2akit.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/a2akit/a2akit/issues"
  },
  "homepage": "https://github.com/a2akit/a2akit/tree/main/packages/core#readme",
  "keywords": [
    "a2a",
    "agent",
    "ai",
    "protocol",
    "decorators",
    "typescript",
    "json-rpc",
    "sse",
    "agent-to-agent"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "reflect-metadata": "^0.2.0"
  },
  "peerDependenciesMeta": {
    "reflect-metadata": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "reflect-metadata": "^0.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
